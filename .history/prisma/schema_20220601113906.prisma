// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Spawn {
  id String
  ownerid: ID
  birthdate: Int
  chain: String
  class: String
  name: String
  breedcount: Int
  figures: String
  createdAt: Int
  updatedAt: Int
  tokenId: Int
  price: Float
  deleteCount: Int
}